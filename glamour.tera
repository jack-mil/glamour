---
whiskers:
  version: 2.5.1
  matrix:
    - flavor
  filename: "themes/catppuccin-{{ flavor.identifier }}.json"
  hex_format: "#{{r}}{{g}}{{b}}"
---
{
  "document": {
    "block_prefix": "\n",
    "block_suffix": "\n",
    "color": "{{ text.hex }}",
    "margin": 2
  },
  "block_quote": {
    "indent": 1,
    "indent_token": "â”‚ ",
    "background_color": "{{ surface0.hex }}"
  },
  "paragraph": {},
  "list": {
    "level_indent": 2
  },
  "heading": {
    "block_suffix": "\n",
    "color": "{{ text.hex }}",
    "bold": true
  },
  "h1": {
    "prefix": "â–“â–“â–“â–“â–“â–“ ",
    "suffix": " â–“â–“â–“â–“â–“â–“",
    "color": "{{ red.hex }}"
  },
  "h2": {
    "prefix": "â–“â–“â–“â–“â–“ ",
    "suffix": " â–“â–“â–“â–“â–“",
    "color": "{{ peach.hex }}"
  },
  "h3": {
    "prefix": "â–“â–“â–“â–“ ",
    "suffix": " â–“â–“â–“â–“",
    "color": "{{ yellow.hex }}"
  },
  "h4": {
    "prefix": "â–“â–“â–“ ",
    "suffix": " â–“â–“â–“",
    "color": "{{ green.hex }}",
    "bold": true
  },
  "h5": {
    "prefix": "~~ ",
    "suffix": " ~~",
    "color": "{{ sky.hex }}"
  },
  "h6": {
    "prefix": "~ ",
    "suffix": " ~",
    "color": "{{ lavender.hex }}"
  },
  "text": {},
  "strikethrough": {
    "crossed_out": true
  },
  "emph": {
    "italic": true,
    "color": "{{ flamingo.hex }}"
  },
  "strong": {
    "bold": true,
    "color": "{{ flamingo.hex }}"
  },
  "hr": {
    "color": "{{ overlay0.hex }}",
    "format": "\n--------\n",
    "bold": true
  },
  "item": {
    "block_prefix": "â€¢ "
  },
  "enumeration": {
    "block_prefix": ". "
  },
  "task": {
    "ticked": "[âœ“] ",
    "unticked": "[ ] "
  },
  "link": {
    "color": "{{ blue.hex }}",
    "underline": true,
    "block_prefix": "(",
    "block_suffix": ")"
  },
  "link_text": {
    "color": "{{ sapphire.hex }}",
    "bold": true
  },
  "image": {
    "color": "{{ blue.hex }}",
    "underline": true
  },
  "image_text": {
    "color": "{{ sapphire.hex }}",
    "format": "Image: {{ "{{.text}}" }} â†’"
  },
  "code": {
    "prefix": " ",
    "suffix": " ",
    "color": "{{ teal.hex }}",
    "background_color": "{{ mantle.hex }}"
  },
  "code_block": {
    "color": "{{ mantle.hex }}",
    "margin": 2,
    "chroma": {
      "text": {
        "color": "{{ text.hex }}"
      },
      "error": {
        "color": "{{ red.hex }}",
        "background_color": "{{ crust.hex }}"
      },
      "comment": {
        "color": "{{ overlay0.hex }}"
      },
      "comment_preproc": {
        "color": "{{ blue.hex }}"
      },
      "keyword": {
        "color": "{{ mauve.hex }}"
      },
      "keyword_reserved": {
        "color": "{{ mauve.hex }}"
      },
      "keyword_namespace": {
        "color": "{{ yellow.hex }}"
      },
      "keyword_type": {
        "color": "{{ yellow.hex }}"
      },
      "operator": {
        "color": "{{ sky.hex }}"
      },
      "punctuation": {
        "color": "{{ overlay2.hex }}"
      },
      "name": {
        "color": "{{ lavender.hex }}"
      },
      "name_builtin": {
        "color": "{{ peach.hex }}"
      },
      "name_tag": {
        "color": "{{ mauve.hex }}"
      },
      "name_attribute": {
        "color": "{{ yellow.hex }}"
      },
      "name_class": {
        "color": "{{ yellow.hex }}"
      },
      "name_constant": {
        "color": "{{ yellow.hex }}"
      },
      "name_decorator": {
        "color": "{{ pink.hex }}"
      },
      "name_exception": {},
      "name_function": {
        "color": "{{ blue.hex }}"
      },
      "name_other": {},
      "literal": {},
      "literal_number": {
        "color": "{{ peach.hex }}"
      },
      "literal_date": { 
        "color": "{{ pink.hex }}"
      },
      "literal_string": {
        "color": "{{ green.hex }}"
      },
      "literal_string_escape": {
        "color": "{{ pink.hex }}"
      },
      "generic_deleted": {
        "color": "{{ red.hex }}"
      },
      "generic_emph": {
        "color": "{{ text.hex }}",
        "italic": true
      },
      "generic_inserted": {
        "color": "{{ green.hex }}"
      },
      "generic_strong": {
        "color": "{{ text.hex }}",
        "bold": true
      },
      "generic_subheading": {
        "color": "{{ sky.hex }}"
      },
      "background": {
        "background_color": "{{ mantle.hex }}"
      }
    }
  },
  "table": {
    "center_separator": "â”¼",
    "column_separator": "â”‚",
    "row_separator": "â”€"
  },
  "definition_list": {},
  "definition_term": {
    "color": "{{ green.hex }}"
  },
  "definition_description": {
    "block_prefix": "\nðŸ ¶ "
  },
  "html_block": {},
  "html_span": {}
}
